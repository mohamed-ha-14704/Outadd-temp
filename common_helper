class Util{static isValidGUID(a){return/^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$/.test(a)}static convertStringToGuidFormat(a){a=a.replace(/([0-z]{8})([0-z]{4})([0-z]{4})([0-z]{4})([0-z]{12})/,"$1-$2-$3-$4-$5");return 36==a.length?a:null}}
class outlookJSAPIHelper{static isMIPFunctionalitySupported(){return Office.context.requirements.isSetSupported("Mailbox","1.13")}static getAttachmentIdList(a){return new Promise(e=>{let c=[];try{a.getAttachmentsAsync(b=>{if(b.status===Office.AsyncResultStatus.Succeeded&&(b=b.value,0<b.length))for(let d=0;d<b.length;d++)c.push(b[d].id);e(c)})}catch(b){logError("Error while fetching attachment Ids"+b),e(c)}})}};
